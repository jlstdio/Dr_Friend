<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.joonheelee.dr_friend">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SignUp"></activity>

        <activity android:name=".Login"></activity>

        <activity android:name="Profile"></activity>

        <activity android:name=".workout.Walking"></activity>

        <activity android:name=".workout.Running"></activity>

        <activity android:name=".workout.Treadmill"></activity>

        <activity android:name=".workout.Bike"></activity>

    </application>

</manifest>